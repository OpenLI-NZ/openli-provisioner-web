#!/bin/sh
# prerm script for openli-provisioner-web
#
# see: dh_installdeb(1)

set -e

case "$1" in
    remove|upgrade|deconfigure)
        /usr/bin/openli-web-provisioner-uninstall-deb.sh
        ;;
    failed-upgrade)
        ;;

    *)
        echo "postinst called with unknown argument \`$1'" >&2
        exit 1
        ;;
esac

#DEBHELPER#

exit 0
